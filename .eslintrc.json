{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"eslint:recommended"
	],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": [
		"react",
		"react-native"
	],
	"rules": {
		"react/prop-types": "off",
		"no-unused-vars": "off"
	}
}